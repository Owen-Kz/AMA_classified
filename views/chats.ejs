            <!-- End Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <!-- ============================================================== -->
            <!-- Container fluid  -->
            <!-- ============================================================== -->
            <div class="container-fluid">
                <!-- ============================================================== -->
                <!-- Three charts -->
                <!-- ============================================================== -->
            
                <div class="container">
                    <div class="row no-gutters">
                      <div class="col-md-4 border-right chatListContainer" id="chaListContainer">
                        <div class="settings-tray">
                          <img class="profile-image" src="/plugins/images/users1.jpg" alt="Profile img">
                          <span class="settings-tray--right">
                            <!-- <i class="material-icons">cached</i> -->
                            <!-- <i class="material-icons">message</i> -->
                            <i class="material-icons menuHamburger">menu</i>
                          </span>
                        </div>
                        <div class="search-box">
                          <div class="input-wrapper">
                            <i class="material-icons">search</i>
                            <input placeholder="Search here" type="text">
                          </div>
                        </div>
                        </div>
                        
                     
                       
                
                    
                      <div class="col-md-8" id="messageContainer">
                        <div class="settings-tray" >
                            <div class="friend-drawer no-gutters friend-drawer--grey" >
                            <div class="friend-drawer no-gutters friend-drawer--grey" id="messageHeaderTop" style="width: 80%;">
                                </div>
                               
                                <span class="settings-tray--right">
                                  <!-- <i class="material-icons">cached</i> -->
                                  
                                  <i class="material-icons menuHamburger">menu</i>
                                </span>
                            </div>
            
                        </div>
                        <div class="chat-panel">
                            <div class="mainMessageBody" id="mainMessageBody">
                       
                         
                      
                        
                      
                          </div>
                          <div class="row" id="inputFieldContainer">
                            <div class="col-12">
                              <div class="chat-box-tray">
                                <i class="material-icons">sentiment_very_satisfied</i>
                                <form id="message-form" style="width: 100%;">

                    
                                <textarea type="text" id="message-input" placeholder="Type your message here..."></textarea>
                                <input type="submit" value="submit" hidden id="submit">
                               </form>
                                <i class="material-icons">mic</i>
                                <i class="material-icons" id="send">send</i>
                              </div>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                <!-- partial -->
                   
                    

